{
  "title": "Launch apps",
  "rules": [
    {
      "description": "Launch apps by right control+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/System/Applications/Utilities/Activity Monitor.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "caps_lock"
